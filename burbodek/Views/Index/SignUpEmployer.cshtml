	<script>
		document.addEventListener('DOMContentLoaded', function() {

			// Get form elements
			const trainingCheckbox = document.getElementById('kt_create_account_form_account_type_personal');
			const employerCheckbox = document.getElementById('kt_create_account_form_account_type_corporate');

			// Store original field HTML for restoration if needed
			let originalTrainingFields = [];
			let originalEmployerFields = [];

			// Get all field containers in Step 4 and store their HTML
			const trainingFields = document.querySelectorAll('[data-account="training"]');
			const employerFields = document.querySelectorAll('[data-account="employer"]');

			// Store original HTML content
			trainingFields.forEach(field => {
				originalTrainingFields.push(field.cloneNode(true));
			});

			employerFields.forEach(field => {
				originalEmployerFields.push(field.cloneNode(true));
			});

			// Function to toggle field visibility and requirements
			function toggleFields() {
				const isTrainingSelected = trainingCheckbox.checked;
				const isEmployerSelected = employerCheckbox.checked;
				const step4Container = document.querySelector('[data-kt-stepper-element="content"]:nth-child(4) .w-100');

				// Remove all existing dynamic fields first
				const existingTrainingFields = document.querySelectorAll('[data-account="training"]');
				const existingEmployerFields = document.querySelectorAll('[data-account="employer"]');

				existingTrainingFields.forEach(field => field.remove());
				existingEmployerFields.forEach(field => field.remove());

				// Add back only the needed fields
				if (isTrainingSelected) {
					originalTrainingFields.forEach(originalField => {
						const clonedField = originalField.cloneNode(true);
						const input = clonedField.querySelector('input[type="file"]');
						const label = clonedField.querySelector('label');

						// Make required and visible
						clonedField.style.display = 'flex';
						input.setAttribute('required', 'required');
						if (label && !label.classList.contains('required')) {
							label.classList.add('required');
						}

						step4Container.appendChild(clonedField);
					});
				}

				if (isEmployerSelected) {
					originalEmployerFields.forEach(originalField => {
						const clonedField = originalField.cloneNode(true);
						const input = clonedField.querySelector('input[type="file"]');
						const label = clonedField.querySelector('label');

						// Make required and visible
						clonedField.style.display = 'flex';
						input.setAttribute('required', 'required');
						if (label && !label.classList.contains('required')) {
							label.classList.add('required');
						}

						step4Container.appendChild(clonedField);
					});
				}

				// Update form validation if using FormValidation plugin
				if (typeof FormValidation !== 'undefined' && window.stepperFormValidator) {
					// Revalidate the form to account for new required fields
					if (isTrainingSelected) {
						setTimeout(() => {
							window.stepperFormValidator.revalidateField('sec_dti');
							window.stepperFormValidator.revalidateField('bir_certificate');
							window.stepperFormValidator.revalidateField('business_permit');
						}, 100);
					}
					if (isEmployerSelected) {
						setTimeout(() => {
							window.stepperFormValidator.revalidateField('poea_license');
							window.stepperFormValidator.revalidateField('proof_partnership');
						}, 100);
					}
				}
			}

			// Function to handle checkbox behavior (allow multiple selections)
			function handleCheckboxSelection() {
				// Both checkboxes can be selected simultaneously
				// No need to uncheck the other when one is selected
				toggleFields();

				// Update Step 2 visibility based on selections
				updateStep2Visibility();
			}

			// Function to update Step 2 content based on selection
			function updateStep2Visibility() {
				const step2 = document.querySelector('[data-kt-stepper-element="content"]:nth-child(2)');
				const accountNameField = step2?.querySelector('[name="account_name"]');
				const accountNameLabel = step2?.querySelector('label[class*="form-label"]');

				if (accountNameField && accountNameLabel) {
					const isEmployerSelected = employerCheckbox.checked;

					if (isEmployerSelected) {
						accountNameLabel.textContent = 'Name of Employer';
						accountNameField.placeholder = 'Enter employer name';
					} else {
						accountNameLabel.textContent = 'Name of Training Center';
						accountNameField.placeholder = 'Enter training center name';
					}
				}
			}

			// Event listeners for checkboxes
			if (trainingCheckbox) {
				trainingCheckbox.addEventListener('change', handleCheckboxSelection);
			}

			if (employerCheckbox) {
				employerCheckbox.addEventListener('change', handleCheckboxSelection);
			}

			// Initial setup - run on page load
			toggleFields();
			updateStep2Visibility();

			// Additional validation for stepper navigation
			function validateStepTransition(currentStep, nextStep) {
				if (currentStep === 1 && nextStep === 2) {
					// Validate that at least one account type is selected
					const isTrainingSelected = trainingCheckbox.checked;
					const isEmployerSelected = employerCheckbox.checked;

					if (!isTrainingSelected && !isEmployerSelected) {
						// Show error message using Metronic's toast or alert
						if (typeof Swal !== 'undefined') {
							Swal.fire({
								text: "Please select at least one account type.",
								icon: "error",
								buttonsStyling: false,
								confirmButtonText: "Ok, got it!",
								customClass: {
									confirmButton: "btn btn-primary"
								}
							});
						} else {
							alert('Please select at least one account type.');
						}
						return false;
					}
				}

				if (currentStep === 4) {
					// Validate required files are uploaded
					let hasError = false;
					const errorMessages = [];

					// Get current fields in DOM (not the original stored ones)
					const currentTrainingFields = document.querySelectorAll('[data-account="training"]');
					const currentEmployerFields = document.querySelectorAll('[data-account="employer"]');

					if (trainingCheckbox.checked) {
						currentTrainingFields.forEach(field => {
							const input = field.querySelector('input[type="file"]');
							const label = field.querySelector('label');
							if (input.hasAttribute('required') && !input.files.length) {
								hasError = true;
								errorMessages.push(`${label.textContent} is required`);
							}
						});
					}

					if (employerCheckbox.checked) {
						currentEmployerFields.forEach(field => {
							const input = field.querySelector('input[type="file"]');
							const label = field.querySelector('label');
							if (input.hasAttribute('required') && !input.files.length) {
								hasError = true;
								errorMessages.push(`${label.textContent} is required`);
							}
						});
					}

					if (hasError) {
						if (typeof Swal !== 'undefined') {
							Swal.fire({
								text: errorMessages.join('\n'),
								icon: "error",
								buttonsStyling: false,
								confirmButtonText: "Ok, got it!",
								customClass: {
									confirmButton: "btn btn-primary"
								}
							});
						} else {
							alert(errorMessages.join('\n'));
						}
						return false;
					}
				}

				return true;
			}

			// Expose validation function globally for stepper integration
			window.validateStepTransition = validateStepTransition;

			// If using KTStepper, integrate with existing stepper instance
			if (typeof KTStepper !== 'undefined') {
				// Wait for stepper to be initialized
				setTimeout(() => {
					const stepperElement = document.querySelector('[data-kt-stepper="true"]');
					if (stepperElement && stepperElement.stepper) {
						const stepper = stepperElement.stepper;

						// Override the stepper's next function
						const originalNext = stepper.goNext;
						stepper.goNext = function() {
							const currentIndex = stepper.getCurrentStepIndex();
							const nextIndex = currentIndex + 1;

							if (validateStepTransition(currentIndex, nextIndex)) {
								originalNext.call(this);
							}
						};
					}
				}, 100);
			}

			// Debug function to check current state
			window.debugFormState = function() {
				console.log('Training Center selected:', trainingCheckbox.checked);
				console.log('Employer selected:', employerCheckbox.checked);
				console.log('Training fields visible:', Array.from(trainingFields).map(f => f.style.display !== 'none'));
				console.log('Employer fields visible:', Array.from(employerFields).map(f => f.style.display !== 'none'));
			};

		});
	</script>


